{
  "name": "zingchart-static",
  "siteName": "ZingChart",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "NODE_ENV=development npm run build && node scripts/dev.js",
    "dev:prod": "NODE_ENV=production npm run build && http-server ./dist -p $npm_package_config_devPort",
    "reset": "rm -rf node_modules && rm -rf dist && npm install"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "devPort": 3000
  },
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "chalk": "^2.4.1",
    "clean-css": "^4.2.1",
    "fs-extra": "^7.0.1",
    "html-minifier": "^3.5.21",
    "http-server": "^0.11.1",
    "jsdom": "^13.2.0",
    "rimraf": "^2.6.2",
    "uglify-es": "^3.3.9"
  }
}
